      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def f_b(x):\n",
        "    return np.exp(x) + x\n",
        "\n",
        "y_b = f_b(x)\n",
        "\n",
        "plt.figure(figsize=(8, 6))\n",
        "plt.plot(x, y_b, label='f(x) = $e^x + x$')\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('f(x)')\n",
        "plt.title('Gráfico da função f(x)')\n",
        "plt.grid(True)\n",
        "plt.legend()\n",
        "plt.axhline(0, color='black',linewidth=0.5)\n",
        "plt.axvline(0, color='black',linewidth=0.5)\n",
        "\n",
        "plt.scatter([-1], [f_b(-1)], color='red', label='Ponto crítico')\n",
        "plt.legend()\n",
        "plt.show()\n"
